<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>tillLunch</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>lunch</display-name>
    <servlet-name>lunch</servlet-name>
    <servlet-class>com.acme.LunchServlet</servlet-class>
    <init-param>
      <description></description>
      <param-name>eventName</param-name>
      <param-value>Lunch</param-value>
    </init-param>
    <init-param>
      <description></description>
      <param-name>occures</param-name>
      <param-value>2012-09-24 12:00</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>lunch</servlet-name>
    <url-pattern>/lunch</url-pattern>
    <url-pattern>/foo/bar/*</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>ContentTypeFilter</display-name>
    <filter-name>ContentTypeFilter</filter-name>
    <filter-class>com.acme.ContentTypeFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>ContentTypeFilter</filter-name>
    <url-pattern>/*</url-pattern>
    <dispatcher>REQUEST</dispatcher>
    <dispatcher>FORWARD</dispatcher>
    <dispatcher>INCLUDE</dispatcher>
    <dispatcher>ERROR</dispatcher>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.acme.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CoffeList</display-name>
    <servlet-name>CoffeList</servlet-name>
    <servlet-class>com.acme.CoffeList</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CoffeList</servlet-name>
    <url-pattern>/coffeelist</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>com.acme.StartupListener</listener-class>
  </listener>
</web-app>